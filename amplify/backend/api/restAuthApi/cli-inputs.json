{
  "version": 1,
  "paths": {
    "/hello": {
      "name": "/hello",
      "lambdaFunction": "restAuth",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/list-lawyers": {
      "name": "/list-lawyers",
      "lambdaFunction": "ListLawyers",
      "permissions": {
        "setting": "open"
      }
    },
    "/create-user": {
      "name": "/create-user",
      "lambdaFunction": "AdvoCreateUser",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    },
    "/current-user": {
      "name": "/current-user",
      "lambdaFunction": "AdvoGetCurrentAuthUser",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    }
  }
}